    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@types/ioredis": "^4.28.10",
    "@types/lodash": "^4.14.178",
    "@types/ws": "^8.5.3",
    "fastify": "^3.25.3",
